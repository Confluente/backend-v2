{
  "name": "hsaconfluente.nl-backend",
  "version": "0.1.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "npm run start_dev",
    "start_dev": "cross-env NODE_ENV=development nodemon --legacy-watch --watch ./src -e ts --exec ts-node ./src/index.ts",
    "start_prod": "cross-env NODE_ENV=production nodemon --watch ./src -e ts --exec ts-node ./src/index.ts",
    "export": "node export.ts",
    "gendb": "npx ts-node ./src/import_initial.ts",
    "tests": "cross-env NODE_ENV=test mocha -r ts-node/register test/**/*.test.ts test/*.test.ts test/**/**/*.test.ts -t 10000 --require test/testFactory.ts --exit"
  },
  "author": "H.S.A. Confluente",
  "license": "",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crypto-ts": "^1.0.2",
    "express": "^4.18.2",
    "marked": "^4.2.4",
    "mime-types": "^2.1.35",
    "mocha": "^10.2.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "q": "^1.5.1",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.9",
    "@types/marked": "^4.0.8",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.17",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.7",
    "@types/q": "^1.5.5",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.7",
    "codelyzer": "^6.0.2",
    "tslint": "^6.1.3"
  }
}
