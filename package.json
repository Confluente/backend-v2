  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "npm run start_dev",
    "start_dev": "cross-env NODE_ENV=development nodemon --watch ./src/*ts --exec ts-node ./src/index.ts",
    "start_prod": "cross-env NODE_ENV=production nodemon --watch ./src/*ts --exec ts-node ./src/index.ts",
    "export": "node export.ts",
    "gendb": "npx ts-node src/import_initial.ts",
    "tests": "cross-env NODE_ENV=test mocha -r ts-node/register test/**/*.test.ts --recursive -t 10000 --require test/testFactory.ts --exit"
  },
  "author": "H.S.A. Confluente",
  "license": "",
